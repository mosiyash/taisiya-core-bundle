{
  "name": "taisiya/core-bundle",
  "license": "MIT",
  "type": "taisiya-bundle",
  "description": "The \"Core Bundle\".",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Taisiya\\CoreBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Taisiya\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "pimple/pimple": "^3.0",
    "symfony/finder": "^3.0",
    "slim/slim": "^3.0",
    "robmorgan/phinx": "^0.6",
    "nikic/php-parser": "^2.1|^3.0"
  },
  "require-dev": {
    "composer/composer": "*"
  },
  "scripts": {
    "post-install-cmd": [
      "Taisiya\\CoreBundle\\Composer\\ScriptHandler::copySettings",
      "Taisiya\\CoreBundle\\Composer\\ScriptHandler::mergeSettings",
      "Taisiya\\CoreBundle\\Composer\\ScriptHandler::createPhinxConfigFile"
    ],
    "post-update-cmd": [
      "Taisiya\\CoreBundle\\Composer\\ScriptHandler::copySettings",
      "Taisiya\\CoreBundle\\Composer\\ScriptHandler::mergeSettings",
      "Taisiya\\CoreBundle\\Composer\\ScriptHandler::createPhinxConfigFile"
    ]
  }
}
